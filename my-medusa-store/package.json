{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "predeploy": "medusa db:migrate",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "user": "medusa user -e admin@medusa-test.com -p supersecret",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/dashboard": "latest",
    "@medusajs/admin-sdk": "2.12.6",
    "@medusajs/auth-google": "^2.13.1",
    "@medusajs/cli": "2.12.6",
    "@medusajs/file-local": "2.12.6",
    "@medusajs/file-s3": "2.12.6",
    "@medusajs/framework": "2.12.6",
    "@medusajs/medusa": "2.12.6",
    "@medusajs/event-bus-redis": "2.12.6",
    "@medusajs/cache-redis": "2.12.6",
    "express": "^4.19.2",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.6",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "resolutions": {
    "minimatch": "3.1.2",
    "brace-expansion": "2.0.1"
  },
  "overrides": {
    "minimatch": "3.1.2",
    "brace-expansion": "2.0.1"
  }
}